<mapper namespace="Board">

	<insert id="insertBoard" parameterType="boardVO">
		INSERT INTO board VALUES(seq_b_no.nextval, sysdate, #{b_content}, #{b_location}, #{b_price}, #{b_name}, #{b_title}, 1, 0, 0,#{b_ea},#{b_latitude},#{b_longitude})
	</insert>
		<!--  #{}: ''(따옴표가 자동으로 붙음)
				${} : "(따옴표가 붙지않음) -->
				

</mapper>