<script>
  $(function(){
	  
	  $('.result-form-item span').hover(function(){
		$(this).toggleClass('active');
		if($(this).hasClass('active')) 
			$(this).css({
			  "animation": "bounce .3s ease infinite alternate"
		  });
	
	  },function(){
		 	$(this).click(function(){
		 		$(this).removeAttr('style').delay(3000);
		 	});

	  });
      $('.result-button').click(function(){
    	  alert("dd");
    	  $(this).css({"width":'73%', "transition":'all 1s ease-in-out'});
      }
        
          
});

    </script>
    
