package com.java.jjam.dao;

import java.util.List;

import org.mybatis.spring.SqlSessionTemplate;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;

import com.java.jjam.domain.ChatVO;



@Repository
public class ChatDAO {

	@Autowired
	private SqlSessionTemplate mybatis;
	
	//쪽지 insert
	
	
	//쪽지 select
	public List<ChatVO> selectChat(ChatVO vo){
		System.out.println("mybatis selectChat 실행");
		List<ChatVO> list = mybatis.selectList("Chat.selectChat", vo);
		System.out.println("챗dao"+list.get(0).getChat_title());
		return list;
	}
	
	
}
