<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Board">

	<insert id="insertBoard" parameterType="boardVO">
		INSERT INTO board VALUES(seq_b_no.nextval, sysdate, #{b_content}, #{b_location}, #{b_price}, #{b_name}, #{b_title}, 1, 0, 0,#{b_ea},#{cate_id},#{c_id})
	</insert>
		<!--  #{}: ''(따옴표가 자동으로 붙음)
				${} : "(따옴표가 붙지않음) -->
				

</mapper>

<mapper namespace="Customer">
	<insert id="customerInsert" parameterType="customerVO" >
	INSERT INTO customer VALUES(#{c_id}, #{c_name}, #{c_tel}, #{c_pw})
	</insert>
	

</mapper>